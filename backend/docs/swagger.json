{
    "schemes": [
        "http",
        "https"
    ],
    "swagger": "2.0",
    "info": {
        "description": "API for downloading videos from social media platforms (Instagram, Twitter/X, YouTube)",
        "title": "VidToGallery API",
        "contact": {
            "name": "VidToGallery API Support",
            "email": "support@vidtogallery.com"
        },
        "version": "1.0.0"
    },
    "host": "localhost:8080",
    "basePath": "/",
    "paths": {
        "/api/v1/process": {
            "post": {
                "description": "Extract video download URL from social media platform",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Video Processing"
                ],
                "summary": "Process video URL",
                "parameters": [
                    {
                        "description": "Video URL to process",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.VideoRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Video processed successfully",
                        "schema": {
                            "$ref": "#/definitions/models.VideoResponse"
                        }
                    },
                    "400": {
                        "description": "Invalid request",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal server error",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/health": {
            "get": {
                "description": "Check if the API is running and healthy",
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Health"
                ],
                "summary": "Health check endpoint",
                "responses": {
                    "200": {
                        "description": "API is healthy",
                        "schema": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "models.ErrorResponse": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "details": {
                    "type": "string"
                },
                "error": {
                    "type": "string"
                }
            }
        },
        "models.VideoRequest": {
            "type": "object",
            "required": [
                "url"
            ],
            "properties": {
                "quality": {
                    "type": "string"
                },
                "url": {
                    "type": "string"
                }
            }
        },
        "models.VideoResponse": {
            "type": "object",
            "properties": {
                "duration": {
                    "type": "integer"
                },
                "metadata": {
                    "type": "object",
                    "additionalProperties": {
                        "type": "string"
                    }
                },
                "platform": {
                    "type": "string"
                },
                "processed_at": {
                    "type": "string"
                },
                "quality": {
                    "type": "string"
                },
                "title": {
                    "type": "string"
                },
                "video_url": {
                    "type": "string"
                }
            }
        }
    }
}